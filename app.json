{
  "expo": {
    "name": "HelloHoji",
    "slug": "HelloHoji",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hoji-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/hoji-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#d3c7b1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hoji-adaptive-icon.png",
        "backgroundColor": "#d3c7b1"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
